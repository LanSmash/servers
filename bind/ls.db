$TTL	604800
@	IN	SOA	ls. root.ls. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN	NS	localhost.
block		IN	CNAME	proxy

; proxy wpad info
wpad            IN      CNAME   proxy
@		IN	TXT	"service: wpad:http://wpad.ls:80/wpad.dat"
wpad.tcp	IN	SRV	0 0 80 wpad.ls

steamcache	IN	CNAME	proxy
cache		IN	CNAME	proxy

;servers 
proxy           IN      A       10.0.0.2
dc              IN      A       10.0.0.48

