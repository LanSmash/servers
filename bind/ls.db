$TTL	604800
@	IN	SOA	ls. root.ls. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN	NS	localhost.
block		IN	CNAME	proxy

